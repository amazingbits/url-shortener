{
  "name": "guilherme/url-shortener",
  "authors": [
    {
      "name": "Guilherme Andrade",
      "email": "gui-keanne@hotmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Src\\": "src/"
    },
    "files": [
      "helpers.php"
    ]
  },
  "scripts": {
    "migrate": "php migration.php",
    "test": "./vendor/bin/pest --display-errors"
  },
  "config": {
    "platform": {
      "php": "8.2.0"
    },
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "require": {
    "php": "8.2.*",
    "ext-json": "*",
    "vlucas/phpdotenv": "5.6.1",
    "illuminate/database": "12.0.1",
    "coffeecode/router": "2.0.2",
    "endroid/qr-code": "6.0.3"
  },
  "require-dev": {
    "pestphp/pest": "3.7.4",
    "mockery/mockery": "1.6.12"
  }
}
